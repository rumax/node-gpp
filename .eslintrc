{
  "ecmaFeatures": {
    "modules": true,
    "jsx": true
  },
  "plugins": [

  ],
  "env": {
    "es6": true,
    "jasmine": true,
    "node": true,
    "mocha": true,
    "browser": true,
    "builtin": true
  },
  "globals": {
    "$": false,
    "moment": false,
    "interact": false,
    "i18n": false,
    "Cookies": false,
    "Highcharts": false,
    "_": false
  },
  "rules": {
    "max-len": [2, 80, 4],
    "id-length": [1,
      {
        "min": 2,
        "max": 25,
        "properties": "always",
        "exceptions": ["i", "j"]
      }
    ],
    "block-scoped-var": 2,
    "camelcase": 2,
    "comma-style": 2,
    "curly": [
      2,
      "all"
    ],
    "dot-notation": 0,
    "eqeqeq": [
      2,
      "allow-null"
    ],
    "guard-for-in": 2,
    "key-spacing": 0,
    "new-cap": 2,
    "no-bitwise": 2,
    "no-caller": 2,
    "no-cond-assign": [
      2,
      "except-parens"
    ],
    "no-debugger": 2,
    "no-empty": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-parens": 0,
    "no-irregular-whitespace": 2,
    "no-iterator": 2,
    "no-loop-func": 2,
    "no-multi-spaces": 0,
    "no-multi-str": 0,
    "no-mixed-spaces-and-tabs": 0,
    "no-new": 2,
    "no-plusplus": 0,
    "no-proto": 2,
    "no-script-url": 2,
    "no-sequences": 2,
    "no-shadow": 2,
    "no-undef": 2,
    "no-underscore-dangle": 0,
    "no-unused-vars": 2,
    "no-use-before-define": [2, "nofunc"],
    "no-with": 2,
    "quotes": [
      2,
      "single"
    ],
    "semi": [2, "always"],
    "no-extra-semi": 2,
    "semi-spacing": [
      1,
      {
        "before": false,
        "after": true
      }
    ],
    "strict": 2,
    "valid-typeof": 2,
    "wrap-iife": [
      2,
      "inside"
    ]
  }
}
